            android:theme="@style/TemaFormularios"
            android:windowSoftInputMode="stateHidden|adjustPan" />
        <activity
            android:name="itspay.br.com.activity.MarketPlaceActivity"
            android:label="@string/title_activity_market_place"
            android:theme="@style/AppTheme" />
        <activity
            android:name="itspay.br.com.activity.PedidoDetalheActivity"
            android:label="Detalhes do Pedido"
            android:theme="@style/TemaFormularios"
            android:windowSoftInputMode="stateHidden|adjustPan" />
        <activity
            android:name="itspay.br.com.activity.ProdutoDetalheActivity"
            android:label="Detalhes do Produto"
            android:theme="@style/TemaFormularios"
            android:windowSoftInputMode="stateHidden|adjustPan" />
        <activity
            android:name="itspay.br.com.activity.EnderecoActivity"
            android:label="Endereços"
            android:noHistory="true"
            android:theme="@style/TemaFormularios"
            android:windowSoftInputMode="stateHidden|adjustPan" />
        <activity
            android:name="itspay.br.com.activity.FormaEnvioActivity"
            android:label="Forma de Envio"
