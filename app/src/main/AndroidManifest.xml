            android:windowSoftInputMode="stateHidden|adjustPan" />
        <activity
            android:name="itspay.br.com.activity.ParcelamentoActivity"
            android:label="Parcelamento"
            android:noHistory="true"
            android:theme="@style/TemaFormularios"
            android:windowSoftInputMode="stateHidden|adjustPan" />
        <activity
            android:name="itspay.br.com.activity.ResumoPedidoActivity"
            android:label="Resumo"
            android:noHistory="true"
            android:theme="@style/TemaFormularios"
            android:windowSoftInputMode="stateHidden|adjustPan" />
        <activity
            android:name="itspay.br.com.activity.PagamentoActivity"
            android:label="Pagamento"
            android:noHistory="true"
            android:theme="@style/TemaFormularios"
            android:windowSoftInputMode="stateHidden|adjustPan"/>
    </application>

</manifest>