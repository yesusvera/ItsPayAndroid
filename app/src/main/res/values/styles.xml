<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#00273f</item>
        <item name="colorPrimaryDark">#00273f</item>
        <item name="colorAccent">#ab772d</item>
    </style>

    <style name="TemaFormularios_ITSPAY" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/primary_bahamas</item>
        <item name="colorPrimaryDark">@color/primary_bahamas</item>
        <item name="colorAccent">@color/primary_bahamas</item>
    </style>


    <style name="TemaFormularios" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#00273f</item>
        <item name="colorPrimaryDark">#00273f</item>
        <item name="colorAccent">#ab772d</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="Divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@color/divider_grey</item>
    </style>
</resources>
